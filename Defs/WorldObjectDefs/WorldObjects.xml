<?xml version="1.0" encoding="utf-8"?>
<Defs>
  
  <WorldObjectDef>
    <defName>SmallMoon</defName>
    <label>small moon</label>
    <description>One of what could be many, many moons orbiting this rimworld.</description>
    <worldObjectClass>ThatsAMoon.WorldObject_SmallMoon</worldObjectClass>
    <texture>UI/smallmoon</texture>
    <useDynamicDrawer>true</useDynamicDrawer>
    <expandingIcon>true</expandingIcon>
    <expandingIconTexture>UI/smallmoon</expandingIconTexture>
    <expandingIconPriority>6</expandingIconPriority>
  </WorldObjectDef>
  
  <WorldObjectDef>
    <defName>GassyMoon</defName>
    <label>gas dwarf</label>
    <description>An extremely rare celestial occurrance, a Gas Dwarf orbits this world just far enough not to be ripped to shreds by tidal forces...somehow...</description>
    <worldObjectClass>ThatsAMoon.WorldObject_GassyMoon</worldObjectClass>
    <texture>UI/gassymoon</texture>
    <useDynamicDrawer>true</useDynamicDrawer>
    <expandingIcon>true</expandingIcon>
    <expandingIconTexture>UI/gassymoon</expandingIconTexture>
    <expandingIconPriority>6</expandingIconPriority>
  </WorldObjectDef>
  
  <WorldObjectDef>
    <defName>DestroyedMoon</defName>
    <label>destroyed moon</label>
    <description>A ruin of a miniature moon, heavily bombarded by debris until its orbit was irrevocably altered, causing it to be pulled apart by tidal forces.</description>
    <worldObjectClass>ThatsAMoon.WorldObject_DestroyedMoon</worldObjectClass>
    <texture>UI/destroyedmoon</texture>
    <useDynamicDrawer>true</useDynamicDrawer>
    <expandingIcon>true</expandingIcon>
    <expandingIconTexture>UI/destroyedmoon</expandingIconTexture>
    <expandingIconPriority>6</expandingIconPriority>
  </WorldObjectDef>
  
  <WorldObjectDef>
    <defName>Asteroid</defName>
    <label>asteroid</label>
    <description>An asteroid, one of what could be many, many tiny satellites orbiting this rimworld.</description>
    <worldObjectClass>ThatsAMoon.WorldObject_Asteroid</worldObjectClass>
    <texture>UI/asteroid</texture>
    <useDynamicDrawer>true</useDynamicDrawer>
    <expandingIcon>true</expandingIcon>
    <expandingIconTexture>UI/asteroid</expandingIconTexture>
    <expandingIconPriority>6</expandingIconPriority>
  </WorldObjectDef>
  
</Defs>